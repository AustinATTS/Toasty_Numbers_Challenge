<?xml version="1.0" encoding="utf-8"?>
<layer-list
    xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:id="@android:id/background">
        <shape>
            <corners android:radius="200.0dip" />
            <stroke android:width="3dp" android:color="@color/white_atts" />
        </shape>
    </item>
    <item android:id="@android:id/progress" android:left="6dp" android:top="6dp" android:right="6dp" android:bottom="6dp">
        <clip>
            <shape>
                <corners android:radius="200.0dip" />
                <solid android:color="@color/white_atts" />
            </shape>
        </clip>
    </item>
</layer-list>